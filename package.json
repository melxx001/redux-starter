{
  "name": "universal-react-redux-starter",
  "version": "0.0.1",
  "description": "Universal React/Redux Starter Example",
  "main": "index.js",
  "scripts": {
    "build": "npm run typings && node_modules/.bin/webpack",
    "clean": "rm -Rf _build",
    "compile": "tsc",
    "cover": "npm run compile && node_modules/.bin/istanbul test node_modules/.bin/tape ./_tsc/**/*.test.js -x **/*.test.js | tap-spec",
    "coverage": "npm run cover --coverage",
    "dev": "npm run build && node_modules/.bin/webpack-dev-server --config webpack.config.dev.js",
    "lint": "npm run lint:js && npm run lint:tsc",
    "lint:js": "node_modules/.bin/eslint .",
    "lint:tsc": "node_modules/.bin/tslint src/**/*.ts{,x}",
    "postinstall": "npm run build",
    "test": "node_modules/.bin/ts-node node_modules/.bin/tape ./**/*.test.ts* | tap-spec",
    "typings": "node_modules/.bin/typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/melxx001/redux-starter.git"
  },
  "keywords": [
    "redux",
    "react",
    "universal",
    "isomorphic",
    "starter",
    "node",
    "es6",
    "es2015",
    "typescript"
  ],
  "author": "Hicham El Hammouchi <hicham.elhammouchi@gmail.com> (http://www.hichamelhammouchi.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/melxx001/redux-starter/issues"
  },
  "homepage": "https://github.com/melxx001/redux-starter#readme",
  "dependencies": {
    "cross-env": "^1.0.8",
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "devDependencies": {
    "babel-register": "^6.9.0",
    "concurrently": "^2.1.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^5.2.2",
    "file-loader": "^0.9.0",
    "istanbul": "^0.4.4",
    "react-addons-test-utils": "^15.1.0",
    "source-map-loader": "^0.1.5",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "ts-loader": "^0.8.2",
    "ts-node": "^0.9.3",
    "tslint": "^3.12.1",
    "typescript": "^1.8.10",
    "typings": "^1.3.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
